{
  "name": "x",
  "private": true,
  "version": "2.2.0",
  "description": "Utility functions for rxjs",
  "publishConfig": {
    "access": "public"
  },
  "keywords": [
    "rxjs",
    "pipe",
    "operator",
    "utils"
  ],
  "scripts": {
    "rollup": "del-cli -f dist && rollup -c --cjs2015 --esm2015 --dts",
    "rollup:watch": "npm run rollup -- --watch",
    "sync-pkg": "node build/rollup/_syncPkg.js --run-sync-pkg",
    "doctoc": "doctoc --github README.md",
    "release": "semantic-release",
    "lint": "eslint . --ext .ts,.js",
    "lint:fix": "npm run lint -- --fix",
    "test": "del-cli coverage && cross-env \"TS_NODE_PROJECT=tsconfig.test.json\" nyc mocha",
    "test:watch": "npm run test -- --watch",
    "typecheck": "tsc --noEmit --outDir dist",
    "typecheck:watch": "npm run typecheck -- --watch",
    "patch-doc-link": "node patch-doc-link.js",
    "typedoc:json": "typedoc src/**/*",
    "typedoc:md": "cross-env TS_NODE_TRANSPILE_ONLY=1 TS_NODE_COMPILER_OPTIONS={\\\"module\\\":\\\"commonjs\\\"} ts-node mkdoc.ts"
  },
  "repository": "git@github.com:Alorel/rxutils.git",
  "devDependencies": {
    "@alorel-personal/conventional-changelog-alorel": "^2.1.3",
    "@alorel/eslint-config-base": "^1.0.18",
    "@alorel/eslint-config-typescript": "^1.0.17",
    "@alorel/rollup-plugin-copy": "^1.0.2",
    "@alorel/rollup-plugin-copy-pkg-json": "^1.0.3",
    "@alorel/rollup-plugin-threaded-terser": "^1.1.0",
    "@rollup/plugin-commonjs": "^22.0.2",
    "@rollup/plugin-node-resolve": "^14.0.0",
    "@rollup/plugin-replace": "^4.0.0",
    "@semantic-release/changelog": "^6.0.1",
    "@semantic-release/exec": "^6.0.3",
    "@semantic-release/git": "^10.0.1",
    "@semantic-release/github": "^8.0.6",
    "@semantic-release/npm": "^9.0.1",
    "@types/chai": "^4.3.3",
    "@types/lodash": "^4.14.184",
    "@types/mocha": "^9.1.1",
    "@types/node": "^18.7.15",
    "@types/pluralize": "^0.0.29",
    "@types/sinon": "^10.0.13",
    "@types/uuid": "^8.3.4",
    "@types/zen-observable": "^0.8.3",
    "@typescript-eslint/eslint-plugin": "^5.36.2",
    "@typescript-eslint/parser": "^5.36.2",
    "chai": "^4.3.6",
    "cross-env": "^7.0.3",
    "del-cli": "^5.0.0",
    "doctoc": "^2.2.0",
    "eslint": "^8.23.0",
    "lazy-get-decorator": "^2.2.0",
    "mocha": "^10.0.0",
    "nyc": "^15.1.0",
    "pluralize": "^8.0.0",
    "rollup": "^2.79.0",
    "rollup-plugin-typescript2": "^0.33.0",
    "rxjs": "^7.5.6",
    "semantic-release": "^19.0.5",
    "sinon": "^14.0.0",
    "source-map-support": "^0.5.21",
    "terser": "^5.15.0",
    "ts-node": "^10.9.1",
    "tslib": "^2.4.0",
    "typedoc": "~0.15.3",
    "typescript": "^4.8.2",
    "uuid": "^9.0.0",
    "zen-observable": "^0.8.15"
  },
  "dependencies": {
    "lodash": "^4.0.0"
  },
  "peerDependencies": {
    "rxjs": "^7.0.0"
  },
  "license": "MIT",
  "main": "index.js",
  "es2015": "_bundle/esm2015/index.js",
  "module": "_bundle/esm2015/index.js",
  "types": "index.d.ts"
}
