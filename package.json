{
  "name": "x",
  "private": true,
  "version": "2.3.0",
  "description": "Utility functions for rxjs",
  "publishConfig": {
    "access": "public"
  },
  "keywords": [
    "rxjs",
    "pipe",
    "operator",
    "utils"
  ],
  "scripts": {
    "rollup": "del-cli -f dist && rollup -c",
    "rollup:watch": "npm run rollup -- --watch",
    "sync-pkg": "node build/rollup/_syncPkg.js --run-sync-pkg",
    "doctoc": "doctoc --github README.md",
    "release": "semantic-release",
    "lint": "eslint . --ext .ts,.js",
    "lint:fix": "npm run lint -- --fix",
    "test": "del-cli coverage && cross-env \"TS_NODE_PROJECT=tsconfig.test.json\" nyc mocha",
    "test:watch": "npm run test -- --watch",
    "typecheck": "tsc --noEmit --outDir dist",
    "typecheck:watch": "npm run typecheck -- --watch",
    "patch-doc-link": "node patch-doc-link.js",
    "typedoc:json": "typedoc src/**/*",
    "typedoc:md": "cross-env TS_NODE_TRANSPILE_ONLY=1 TS_NODE_COMPILER_OPTIONS={\\\"module\\\":\\\"commonjs\\\"} ts-node mkdoc.ts"
  },
  "repository": "git@github.com:Alorel/rxutils.git",
  "devDependencies": {
    "@alorel-personal/conventional-changelog-alorel": "^2.1.3",
    "@alorel/eslint-config-base": "^1.0.18",
    "@alorel/eslint-config-typescript": "^1.0.17",
    "@alorel/rollup-plugin-copy": "^1.0.2",
    "@alorel/rollup-plugin-copy-pkg-json": "^1.0.3",
    "@rollup/plugin-commonjs": "^24.0.0",
    "@rollup/plugin-node-resolve": "^15.0.1",
    "@semantic-release/changelog": "^6.0.2",
    "@semantic-release/exec": "^6.0.3",
    "@semantic-release/git": "^10.0.1",
    "@semantic-release/github": "^8.0.7",
    "@semantic-release/npm": "^9.0.1",
    "@types/chai": "^4.3.4",
    "@types/lodash": "^4.14.191",
    "@types/lodash-es": "^4.17.6",
    "@types/mocha": "^10.0.1",
    "@types/node": "^18.11.17",
    "@types/pluralize": "^0.0.29",
    "@types/sinon": "^10.0.13",
    "@types/uuid": "^9.0.0",
    "@types/zen-observable": "^0.8.3",
    "@typescript-eslint/eslint-plugin": "^5.46.1",
    "@typescript-eslint/parser": "^5.46.1",
    "chai": "^4.3.7",
    "cross-env": "^7.0.3",
    "del-cli": "^5.0.0",
    "doctoc": "^2.2.1",
    "eslint": "^8.33.0",
    "lazy-get-decorator": "^2.2.1",
    "mocha": "^10.2.0",
    "nyc": "^15.1.0",
    "pluralize": "^8.0.0",
    "rollup": "^2.79.1",
    "rollup-plugin-typescript2": "^0.34.1",
    "rxjs": "^7.8.0",
    "semantic-release": "^19.0.5",
    "sinon": "^15.0.1",
    "source-map-support": "^0.5.21",
    "ts-node": "^10.9.1",
    "tslib": "^2.4.1",
    "typedoc": "~0.15.8",
    "typescript": "^4.9.4",
    "uuid": "^9.0.0",
    "zen-observable": "^0.10.0"
  },
  "dependencies": {
    "lodash": "^4.17.21",
    "lodash-es": "^4.17.21"
  },
  "peerDependencies": {
    "rxjs": "^7.0.0",
    "lodash": "^4.0.0",
    "lodash-es": "^4.0.0"
  },
  "license": "MIT",
  "main": "index.js",
  "es2015": "_esm2015/index.js",
  "module": "_esm2015/index.js",
  "types": "index.d.ts"
}
