{
  "name": "@aloreljs/rxutils",
  "version": "1.5.3",
  "description": "Utility functions for rxjs",
  "publishConfig": {
    "access": "public"
  },
  "keywords": [
    "rxjs",
    "pipe",
    "operator",
    "utils"
  ],
  "scripts": {
    "rm:coverage": "rimraf coverage",
    "generate:index-test": "node ./generate-index-test.js",
    "pretest": "concurrently \"npm:rm:coverage\" \"npm:generate:index-test\"",
    "test": "cross-env TS_NODE_PROJECT=tsconfig.test.json nyc mocha",
    "test:watch": "npm --scripts-prepend-node-path=auto run test -- --watch",
    "tslint": "alo tslint -p tsconfig.test.json",
    "tslint:fix": "npm --scripts-prepend-node-path=auto run tslint -- --fix",
    "typecheck": "tsc --noEmit -p tsconfig.cjs.json",
    "typecheck:watch": "npm --scripts-prepend-node-path=auto run typecheck -- --watch",
    "build:cjs": "alo build --targets cjs declaration --tsconfig tsconfig.cjs.json --entry src/index.ts",
    "rm:esm-build": "rimraf src-esm",
    "cp:esm-build": "alo copy-files --from src/* --to src-esm",
    "build:esm:prepare": "node prepare-es-build.js",
    "prebuild:esm": "npm run rm:esm-build && npm run cp:esm-build && npm run build:esm:prepare",
    "postbuild:esm": "npm run rm:esm-build",
    "build:esm": "alo build",
    "build:umd": "webpack",
    "prebuild": "rimraf dist",
    "build": "concurrently \"npm:build:*\"",
    "typedoc:json": "typedoc src/**/*",
    "typedoc:md": "ts-node mkdoc.ts"
  },
  "repository": "git@github.com:Alorel/rxutils.git",
  "devDependencies": {
    "@alorel-personal/build-tools": "4.7.19",
    "@alorel-personal/conventional-changelog-alorel": "2.1.1",
    "@alorel-personal/semantic-release": "1.4.0",
    "@alorel-personal/tslint-rules": "5.0.6",
    "@semantic-release/changelog": "^5.0.0",
    "@semantic-release/exec": "^5.0.0",
    "@semantic-release/git": "^9.0.0",
    "@types/chai": "^4.2.5",
    "@types/fs-extra": "^8.0.0",
    "@types/lodash": "^4.14.149",
    "@types/lodash-es": "^4.17.3",
    "@types/mocha": "^7.0.1",
    "@types/node": "^13.7.4",
    "@types/pluralize": "^0.0.29",
    "@types/rimraf": "^2.0.2",
    "@types/sinon": "^7.5.1",
    "@types/uuid": "^7.0.1",
    "@types/zen-observable": "^0.8.0",
    "bluebird": "^3.7.1",
    "chai": "^4.2.0",
    "concurrently": "^5.0.0",
    "coveralls": "^3.0.9",
    "cross-env": "^7.0.0",
    "fs-extra": "^8.1.0",
    "glob": "^7.1.6",
    "lazy-get-decorator": "^2.2.0",
    "mocha": "^7.0.1",
    "nyc": "^15.0.0",
    "pluralize": "^8.0.0",
    "rimraf": "^3.0.0",
    "rxjs": "^6.5.2",
    "semantic-release": "^17.0.4",
    "sinon": "^9.0.0",
    "source-map-support": "^0.5.12",
    "symbol-observable": "^1.2.0",
    "terser-webpack-plugin": "^2.3.0",
    "ts-loader": "^6.2.1",
    "ts-node": "^8.5.2",
    "tslib": "^1.10.0",
    "typedoc": "~0.15.3",
    "typescript": "^3.7.2",
    "uuid": "^7.0.2",
    "webpack": "^4.41.2",
    "webpack-cli": "^3.3.6",
    "zen-observable": "^0.8.15"
  },
  "dependencies": {
    "lodash": "^4.0.0",
    "lodash-es": "^4.0.0"
  },
  "peerDependencies": {
    "rxjs": "^6.0.0"
  },
  "license": "MIT",
  "main": "index.js",
  "browser": "_bundle/umd.js",
  "jsdelivr": "_bundle/umd.min.js",
  "esm5": "_bundle/esm5/index.js",
  "esm2015": "_bundle/esm2015/index.js",
  "types": "index.d.ts",
  "module": "_bundle/fesm5.js",
  "typings": "index.d.ts"
}
